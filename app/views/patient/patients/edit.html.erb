<div "container">
  <div class='row'>
    <h4>マイページの編集</h4>
  <%=form_with model:@patient, url:patient_patient_path, local:true do |f|%>
    <table>
      <tr>
        <th>名前</th>
        <td><%= f.text_field:name %></td>
      </tr>
      <tr>
        <th>名前(かな)</th>
        <td><%= f.text_field:name_kana %>></td>
      </tr>
      <tr>
        <th>メールアドレス</th>
        <td><%= f.text_field:mail %></td>
      </tr>
      <tr>
        <th>画像</th>
        <td><%=f.attachment_field :image %></td>
      </tr>
    </table>
    　<%= f.submit "変更を保存する"　%>
    <%end%>
    </div>
  </div>